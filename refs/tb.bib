@techreport{Garland95,
	Address = {Pittsburgh, PA, USA},
	Annote = {Our goal is to find a subset S of H which, when triangulated, approximates H as accurately as possible using as few points as possible, and to compute the triangulation as quickly as possible. This is thus an optimization problem. The number of input points in H is n. The number of points in the subset S is m, and consequently the number of vertices in the triangulation is also m. They describe 6 different methods to achieve this. We call refinement algorithms that insert the point(s) of highest error on each pass greedy insertion algorithms, \greedy" because they make irrevocable decisions as they go, and "insertion" because on each pass they insert one or more vertices into the triangulation.},
	Author = {Garland, Michael and Heckbert, Paul S.},
